# Tmux plugins (managed by tpm, not chezmoi)
dot_config/tmux/plugins/
home/dot_config/tmux/plugins/

# Fish shell - dynamically managed config (generated by installer)
.config/fish/conf.d/installer-managed.fish
.config/fish/fish_variables

# Installation logs
install/logs/
install/Install-Logs/
*.log

# Temporary files
*.tmp
*.swp
*.swo
*~

# Sensitive files - SSH keys, credentials, secrets
id_rsa
id_rsa.pub
id_ed25519
id_ed25519.pub
id_ecdsa
id_ecdsa.pub
*.key
*.pem
*.crt
*.p12
*.pfx
.env
.env.*
!.env.example
secrets.json
credentials.json
*_credentials.json
.bitwarden/
bw_session

# Local user config overrides (not tracked)
local/

# User profiles (personal customizations, won't cause merge conflicts)
profiles/*/
!profiles/base/
!profiles/example/
!profiles/dev/

# Development tools
.devcontainer/
.claude/

# Module configs are copied from modules/*/config/ and tracked in git
# This allows fresh installs to work without requiring run_before scripts first
# Only ignore dynamic runtime files within configs:
home/dot_config/fish/fish_variables
home/dot_config/tmux/plugins/**

# User-specific theme selections (symlinks managed by fedpunk theme system)
dot_config/btop/themes/active.theme
dot_config/btop/themes/current.theme
dot_config/hypr/wallpapers/current
dot_config/mako/theme.conf
dot_config/kitty/theme.conf
dot_config/rofi/theme.rasi
dot_config/waybar/theme.css
